{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2022",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "allowJs": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@public/*": [
        "./public/*"
      ],
      "@modules/*": [
        "./node_modules/*"
      ]
    }
  }
}