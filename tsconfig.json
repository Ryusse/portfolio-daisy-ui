{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@public/*": [
        "./public/*"
      ],
      "@modules/*": [
        "./node_modules/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}